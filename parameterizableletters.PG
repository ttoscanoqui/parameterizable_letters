 	C1MOVE .A1
 	C2MOVE .A10_1
	C1MOVE .A2
	C2MOVE .A5
	LMOVE .A6
 	LDEPART 180
 END
 IF .d<=4 THEN
	.d =.d+1
	GOTO FIEC
 ELSE
	IF .d==5 THEN
