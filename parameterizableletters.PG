 POINT .A10 = TRANS(.X+20*.k,.Y-8*.k, .Z, .angO, .angA, .angT)
 POINT .A10_1 = TRANS(.X+15*.k,.Y-8*.k, .Z, .angO, .angA, .angT)
 POINT .A12 = TRANS(.X+15*.k,.Y-12*.k, .Z, .angO, .angA, .angT)

 IF .d==0 THEN
 	JAPPRO .A0,30
 	speed 1 always
 	LMOVE .A0
