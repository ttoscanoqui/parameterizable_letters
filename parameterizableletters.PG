 IF .d==1 THEN
 	JAPPRO .A1,30
 	speed 1 always
	LMOVE .A1
 	LMOVE .A10
 	LMOVE .A6
 	LMOVE .A3
 	LDEPART 30
 	LAPPRO .A0, 20
 	LMOVE .A0
 	LMOVE .A6
 	LDEPART 30
 	LAPPRO .A10, 20
 	LMOVE .A10
 	LMOVE .A2
 	LDEPART 30
