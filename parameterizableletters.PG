.PROGRAM LetraESPOL
 speed 2 always
 inicio1:
 PRINT 2: "ELIGE TAMAÑO NUMÉRICO, SI SELECCIONA 0 SE ACABARÁ EL PROGRAMA"
 PROMPT 2: "INGRESA EL VALOR POR TECLADO ", .k
 inicio:
 .X=-450.326
 .Y=275.337
 .Z=-88.032
 .d=0
 $palabra = ""
 .angA=178.536
 .angO=51.828
 .angT=-124.607
 IF .k < 7 AND .k > 0 THEN 
	;TYPE 2: "TAMAÑO ELEGIDO ", .k
	GOTO lazo23
 ELSE
	IF .K == 0 then
		GOTO fin
	ELSE
		print 2: "TAMAÑO FUERA DE RANGO"
		GOTO inicio1
	END
 END
 lazo23:
 PROMPT 2: "INGRESA e(ESPOL), (f)FIEC, r(REGRESAR) O fin POR TECLADO ", $palabra
 IF $palabra == "e" then
	TYPE 2: "ELEGISTE ESPOL TAMAÑO", .k
	GOTO espol1 
 ELSE
	IF $palabra == "f" then
	TYPE 2: "ELEGISTE FIEC TAMAÑO", .k
	GOTO FIEC1
	ELSE
		IF $palabra == "fin" then
		GOTO fin
		ELSE 
			IF $palabra == "r" then
				GOTO inicio1
			END
		END
	END		
 END
 PRINT 2: "PALABRA NO ADMITIDA"
 GOTO lazo23
 lazo20:


 ESPOL1:
 .Y=.Y+75

 ESPOL:
 .Y=.Y-18*.k
 POINT .A0 = TRANS(.X, .Y , .Z, .angO, .angA, .angT)
 POINT .A1 = TRANS(.X+20*.k,.Y, .Z, .angO, .angA, .angT)
 POINT .A1_10 = TRANS(.X+20*.k,.Y-4*.k, .Z, .angO, .angA, .angT)
 POINT .A2 = TRANS(.X+20*.k,.Y-12*.k, .Z, .angO, .angA, .angT)
 point .A3 = TRANS(.X,.Y-12*.k,.Z, .angO, .angA, .angT)
 point .A6_3 = TRANS(.X,.Y-10*.k,.Z, .angO, .angA, .angT)
 POINT .A4 = TRANS(.X+10*.k,.Y, .Z, .angO, .angA, .angT)
