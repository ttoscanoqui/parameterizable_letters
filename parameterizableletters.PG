 POINT .A8 = TRANS(.X+10*.k,.Y-8*.k, .Z, .angO, .angA, .angT)
 POINT .A8_1 = TRANS(.X+10*.k,.Y-10*.k, .Z, .angO, .angA, .angT)
 POINT .A8_2 = TRANS(.X+10*.k,.Y-6*.k, .Z, .angO, .angA, .angT)
 point .A9 = TRANS(.X+15*.k,.Y,.Z, .angO, .angA, .angT)
 POINT .A10 = TRANS(.X+20*.k,.Y-8*.k, .Z, .angO, .angA, .angT)
 POINT .A12 = TRANS(.X+15*.k,.Y-12*.k, .Z, .angO, .angA, .angT)
 IF .d==0 THEN
 	JAPPRO .A3,30
 	speed 1 always
	LMOVE .A3
 	LMOVE .A0
 	LMOVE .A1
 	LMOVE .A2
 	LDEPART 30
 	LAPPRO .A4, 20
 	LMOVE .A4
 	LMOVE .A8
 	LDEPART 30
 END
 IF .d==1 THEN
 	JAPPRO .A0,20
 	speed 1 always
 	LMOVE .A0
	LMOVE .A6_3
	C1MOVE .A7
	C2MOVE .A8_1
	LMOVE .A8_2
	C1MOVE .A9
	C2MOVE .A1_10
	LMOVE .A2
 	LDEPART 30	
 END
 IF .d==2 THEN 
 	JAPPRO .A0,20
 	speed 1 always
 	LMOVE .A0
	accuracy 25
 	LMOVE .A1
	LMOVE .A10
 	C1MOVE .A12
	C2MOVE .A4
	LDEPART 30	
 END
 IF .d==3 THEN  
	JAPPRO .A6,20
	speed 1 always
	LMOVE .A6
	C1MOVE .A5
	C1MOVE .A10
	C1MOVE .A4
	C2MOVE .A6
	LDEPART 30 	
 END
 IF .d==4 THEN 
 	JAPPRO .A3,30
 	speed 1 always
 	LMOVE .A3
 	LMOVE .A0
	LMOVE .A1
	LDEPART 180	
 END
 IF .d<=4 THEN
	.d =.d+1
	GOTO ESPOL
 ELSE
	IF .d==5 THEN
		.d=0
		GOTO inicio1
	END
 END

 FIEC1:
